{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_astro\\/ec\\.aan0p\\.css$",
			"dest": "_render"
		},
		{
			"src": "^\\/_astro\\/ec\\.dy9ns\\.js$",
			"dest": "_render"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/\\.\\/lunaria\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/robots\\.txt\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		},
		{
			"src": "/.*",
			"dest": "/404.html",
			"status": 404
		}
	]
}