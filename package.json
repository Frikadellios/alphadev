{
  "name": "alphadev",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "bunx --bun astro dev --verbose",
    "start": "bun astro check --verbose && bunx --bun astro dev --watch",
    "build": "bun astro check && bunx --bun astro build --verbose",
    "preview": "bunx --bun astro preview --watch",
    "astro": "astro",
    "lunaria:preview": "lunaria preview",
    "lunaria:build": "lunaria build",
    "format": "bunx @biomejs/biome format ./src/* --write",
    "lint": "bunx @biomejs/biome lint ./src/*",
    "check": "bunx @biomejs/biome check --apply ./src/*"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.9",
    "@astrojs/mdx": "^2.2.0",
    "@astrojs/node": "^8.2.5",
    "@astrojs/partytown": "^2.0.4",
    "@astrojs/rss": "^4.0.5",
    "@astrojs/sitemap": "^3.1.1",
    "@astrojs/starlight": "^0.21.1",
    "@astrojs/starlight-tailwind": "^2.0.1",
    "@astrojs/svelte": "^5.2.0",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/ts-plugin": "^1.6.1",
    "@expressive-code/plugin-collapsible-sections": "^0.33.5",
    "@fontsource/geist-sans": "^5.0.2",
    "@lottiefiles/svelte-lottie-player": "^0.3.1",
    "@lunariajs/core": "^0.0.31",
    "@lunariajs/starlight": "^0.0.5",
    "@tailwindcss/cli": "^4.0.0-alpha.9",
    "@tailwindcss/postcss": "^4.0.0-alpha.9",
    "@tailwindcss/vite": "^4.0.0-alpha.9",
    "astro": "^4.5.6",
    "astro-expressive-code": "^0.33.5",
    "astro-live-code": "^0.0.2",
    "brefer": "^1.0.0-alpha.5.1",
    "lightningcss": "^1.24.1",
    "starlight-blog": "^0.5.1",
    "svelte": "5.0.0-next.80",
    "tailwindcss": "^4.0.0-alpha.9",
    "tiny-invariant": "^1.3.3",
    "typescript": "^5.4.2"
  },
  "peerDependencies": {
    "@tailwindcss/cli": "^4.0.0-alpha.9",
    "@tailwindcss/postcss": "^4.0.0-alpha.9",
    "@tailwindcss/vite": "^4.0.0-alpha.9",
    "svelte": "^5.0.0-next.80",
    "tailwindcss": "^4.0.0-alpha.9"
  },
  "overrides": {
    "astro-expressive-code": "latest"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.3",
    "@types/bun": "latest",
    "@types/node": "^20",
    "mdsvex": "^0.11.0",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-icons": "^0.18.5"
  },
  "main": "astro.config.mjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Frikadellios/alphadev.git"
  },
  "author": "Hrihorii Ilin <devopsick@pm.me>",
  "license": "MIT",
  "description": "This is Crazy Theme for crazy people... I am add svelte runes without runes...",
  "bugs": {
    "url": "https://github.com/Frikadellios/alphadev/issues"
  },
  "homepage": "https://github.com/Frikadellios/alphadev#readme",
  "keywords": [
    "svx",
    "mdx",
    "astro",
    "svelte",
    "tailwind",
